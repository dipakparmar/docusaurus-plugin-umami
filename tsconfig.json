{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": false,
    "esModuleInterop": true,
    "importHelpers": true,
    "jsx": "preserve",
    "moduleResolution": "Node16",
    "module": "Node16",
    "outDir": "dist",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "types": ["@docusaurus/module-type-aliases"]
  },
  "include": ["src"],
}
